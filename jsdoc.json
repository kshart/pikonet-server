{
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": ["./src", "./README.md"],
    "includePattern": ".js$"
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/jsdoc-babel"
  ],
  "opts": {
    "template": "node_modules/tui-jsdoc-template",
    "encoding": "utf8",
    "destination": "doc/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "name": "Pikonet Server",
    "footerText": "Pikonet Server",
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
