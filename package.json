{
  "name": "pikonet-server",
  "version": "1.0.0",
  "description": "Pikonet server",
  "main": "index.js",
  "scripts": {
    "dev": "npx nodemon index.js",
    "build": "npx babel src -d dist -s",
    "serve": "npx babel-node index.js",
    "test": "mocha --require @babel/register",
    "generate-docs": "rd /s /q docs && npx jsdoc -c jsdoc.json"
  },
